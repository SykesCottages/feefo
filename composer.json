{
    "name": "sykescottages/feefo",
    "description": "Gets Feefo reviews from their API",
    "authors": [
        {
            "name": "Elliot Rodgers",
            "email": "elliot.rodgers@sykescottages.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "SykesCottages\\Feefo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SykesCottages\\Feefo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5",
        "doctrine/coding-standard": "^8.2"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
